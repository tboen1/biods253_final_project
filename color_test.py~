import unittest

class colors_test(unittest.TestCase):
    def test_simple(self):
        self.assertEqual(src.utils.guess2color("crane", "grace"), "XGGYG")
    
    def test_duplicate(self):
        self.assertEqual(src.utils.guess2color("child", "couch"), "GXXXY")

if __name__ == "__main__":
    unittest.main()
